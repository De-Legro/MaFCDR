<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="BM2\SiteBundle\Entity\SettlementType" table="types.settlementtype">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="subtype" type="string"/>
		<many-to-one field="upgrades" target-entity="FeatureType" inversed-by="upgrades_to"/>
	</entity>
</doctrine-mapping>
